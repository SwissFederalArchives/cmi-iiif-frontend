  },
  "scripts": {
    "dev": "yarn start",
    "remove_locales": "shx rm -rf ./public/vendor/current/locales/",
    "remove_icons": "shx rm -rf ./public/vendor/current/img/ico/",
    "remove_js": "shx rm -rf ./public/vendor/current/js/",
    "remove_fonts": "shx rm -rf ./public/vendor/current/fonts/",
    "remove_style": "shx rm -rf ./public/vendor/current/css/",
    "copy_eth_locales": "shx cp -r ./public/vendor/eth/locales/ ./public/vendor/current/",
    "copy_eth_style": "shx cp -r ./public/vendor/eth/css/ ./public/vendor/current/css/",
    "copy_eth_icons": "shx cp -r ./public/vendor/eth/img/ico/ ./public/vendor/current/img/",
    "copy_eth_logo": "shx cp ./public/vendor/eth/img/logo.svg ./src/images/logo.svg",
    "copy_eth_env": "shx cp ./public/vendor/eth/.env ./.env",
    "copy_eth_js": "shx cp -r ./public/vendor/eth/js/ ./public/vendor/current/",
    "copy_federation_locales": "shx cp -r ./public/vendor/federation/locales/ ./public/vendor/current/",
    "copy_federation_style": "shx cp -r ./public/vendor/federation/css/ ./public/vendor/current/css/",
    "copy_federation_icons": "shx cp -r ./public/vendor/federation/img/ico/ ./public/vendor/current/img/",
    "copy_federation_logo": "shx cp ./public/vendor/federation/img/logo.svg ./src/images/logo.svg",
    "copy_federation_env": "shx cp ./public/vendor/federation/.env ./.env",
    "copy_federation_js": "shx cp -r ./public/vendor/federation/js/ ./public/vendor/current/",
    "copy_federation_fonts": "shx cp -r ./public/vendor/federation/fonts/ ./public/vendor/current/",
    "start": "webpack-dev-server --open --env development --mode development --hot",
    "build": "webpack --env --mode production",
    "build:generic": "webpack --env cmd='build:generic' --mode production",
    "build:react": "rimraf ./dist/* && tsc -p tsconfig.prod.json && copyfiles -u 1 \"src/**/*.png\" \"src/**/*.css\" \"src/**/*.json\" ./dist/",
    "build:dist": "webpack --mode production && node dist-pkg/buildDistPkg.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "IE 11"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/archival-IIIF/viewer.git"
  },
  "keywords": [
    "IIIF"
  ],
  "author": "Archival IIIF",
  "bugs": {
